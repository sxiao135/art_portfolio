{ 
  "version": 2,

  "builds": [ { 
    "src": "ARTPF/wsgi.py", 
    "use": "@vercel/python", 
    "config": { 
      "maxLambdaSize": "15mb", 
      "runtime": "python3.9" 
    } },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "portfolio/templates/portfolio"
      }}], 

  "routes": [ { 
    "src": "/(.*)", 
    "dest": "ARTPF/wsgi.py"
  } ] ,
  "outputDirectory": "portfolio/templates/portfolio"
}
