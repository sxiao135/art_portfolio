{ 
  "version": 2,

  "builds": [ { 
    "src": "ARTPF/wsgi.py", 
    "use": "@vercel/python", 
    "config": { 
      "maxLambdaSize": "15mb", 
      "runtime": "python3.9" 
    } }, 
    { 
    "src": "build_files.sh", 
      "use": "@vercel/static-build", 
      "config": 
      { 
        "distDir": "assets" 
      } } ], 

  "routes": [ { 
    "src": "/static/(.*)", 
    "dest": "ARTPF/wsgi.py"  
  } ], 

  "outputDirectory": "assets" }
