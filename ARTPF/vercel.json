{ 
  "version": 2,
  "functions": {
    "ARTPF/**/*.py": {
      "excludeFiles": "public/**"
    }
  },
  "builds": [ { 
    "src": "ARTPF/wsgi.py", 
    "use": "@vercel/python", 
    "config": { 
      "maxLambdaSize": "15mb", 
      "runtime": "python3.9" 
    } }, 
    { 
    "src": "build_files.sh", 
      "use": "@vercel/static-build", 
      "config": 
      { 
        "distDir": "assets" 
      } } ], 
  "routes": [ { 
    "src": "/static/(.*)", 
    "dest": "/static/$1" 
  }, 
    { 
      "src": "/(.*)", 
      "dest": "ARTPF/wsgi.py"
    } ], 
  "outputDirectory": "assets" }
