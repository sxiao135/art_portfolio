{ 
  "version": 2,

  "builds": [ { 
    "src": "ARTPF/wsgi.py", 
    "use": "@natanffs/vercel-python-wsgi", 
    "config": { 
      "maxLambdaSize": "15mb", 
      "runtime": "python3.9" 
    } }], 

  "routes": [ { 
    "src": "/(.*)", 
    "dest": "ARTPF/wsgi.py"
  } ] ,
  "outputDirectory": "assets"
}
